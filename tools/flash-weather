#!/bin/bash

PROGRAM=fk-weather-sidecar.bin

if [ ! -f $PROGRAM ]; then
    PROGRAM=build/samd09/modules/weather/sidecar/fk-weather-sidecar.bin
fi

JLinkExe -if SWD -device ATSAMD09D14A -speed 4000 -SelectEmuBySN 801000989 <<EOF
exitonerror 1
connect
sleep 100
r
sleep 100
loadfile $PROGRAM,0x0000
r
q
EOF
